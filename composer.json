{
    "name": "glue-agency/craft-glue-backoffice",
    "description": "Backoffice insights",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "support@glue.be",
        "issues": "https://github.com/glue-agency/craft-glue-backoffice/issues?state=open",
        "source": "https://github.com/glue-agency/craft-glue-backoffice",
        "docs": "https://github.com/glue-agency/craft-glue-backoffice",
        "rss": "https://github.com/glue-agency/craft-glue-backoffice/releases.atom"
    },
    "require": {
        "php": "^7.0 || ^8.0",
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "GlueAgency\\Backoffice\\": "src/"
        }
    },
    "extra": {
        "handle": "glue-backoffice",
        "name": "Glue Backoffice",
        "developer": "Glue Agency",
        "documentationUrl": "https://github.com/glue-agency/craft-glue-backoffice",
        "class": "GlueAgency\\Backoffice\\Backoffice"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
